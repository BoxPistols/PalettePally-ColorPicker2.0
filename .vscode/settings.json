{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#c75d12",
    "titleBar.activeForeground": "#e9d9cc",
    "activityBar.background": "#34312f",
    "activityBar.foreground": "#b5ada6"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.ruby-lsp": true,
    "**/node_modules": true
  },
  "html.completion.attributeDefaultValue": "doublequotes",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single"
}
