{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#d66617",
    "activityBar.background": "#4d3524",
    "titleBar.activeForeground": "#f1ecee",
    "titleBar.inactiveBackground": "#c85909",
    "titleBar.inactiveForeground": "#d1c3c8"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.ruby-lsp": true,
    "**/node_modules": true
  },
  "html.completion.attributeDefaultValue": "doublequotes",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single"
}
