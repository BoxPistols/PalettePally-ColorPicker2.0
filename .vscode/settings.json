{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#e06d1a",
    "titleBar.activeForeground": "#e9d9cc",
    "activityBar.background": "#34312f",
    "activityBar.foreground": "#b5ada6"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.ruby-lsp": true,
    "**/node_modules": true
  },
  "editor.formatOnSave": true,
  "files.autoSave": "afterDelay",
  "files.refactoring.autoSave": false,
  "editorconfig.generateAuto": true,
  "prettier.enable": true,
  "prettier.useEditorConfig": true,
  "commentTranslate.targetLanguage": "ja"
}
